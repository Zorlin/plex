{
    "class": "CommandLineTool",
    "name": "unidock",
    "description": "",
    "baseCommand": ["/bin/bash", "-c"],
    "arguments": [
      "cd /inputs && find -type f -name '*.pdbqt' -print >> outputs/index.txt;",

      ],
    "dockerPull": ,
    "gpuBool": true,
    "networkBool": true,
    "inputs": {
      "protein": {
        "type": "File",
        "glob": ["*.pdbqt"]
      },
      "small_molecule": {
        "type": "File",
        "glob": ["*.pdbqt"]
      },
      "search_mode": {
        "type": "string",
        "default": "balance"
      }
    },
    "outputs": {
      "scores": {
        "type": "File",
        "glob": [""]
      }
    }
}
